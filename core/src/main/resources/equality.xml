<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="equalityPrime.xml"/>

    <bean id="countourEqualityTestWrapper" class="ru.pavelyurkin.musiccomposer.core.equality.melody.EqualNumberOfNotesRequired">
        <property name="equality" ref="contourEquality"/>
    </bean>

    <bean id="inversionEqualityTestWrapper" class="ru.pavelyurkin.musiccomposer.core.equality.melody.EqualNumberOfNotesRequired">
        <property name="equality" ref="inversionEquality"/>
    </bean>

    <bean id="intervalsEqualityTestWrapper" class="ru.pavelyurkin.musiccomposer.core.equality.melody.EqualNumberOfNotesRequired">
        <property name="equality" ref="intervalsEquality"/>
    </bean>

    <bean id="orderEqualityTestWrapper" class="ru.pavelyurkin.musiccomposer.core.equality.melody.EqualNumberOfNotesRequired">
        <property name="equality" ref="orderEquality"/>
    </bean>

    <bean id="rhythmEqualityTestWrapper" class="ru.pavelyurkin.musiccomposer.core.equality.melody.EqualNumberOfNotesRequired">
        <property name="equality" ref="rhythmEquality"/>
    </bean>

</beans>