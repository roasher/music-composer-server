<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="equalityTestPrime.xml"/>

    <bean id="countourEqualityTestWrapper" class="decomposer.analyzer.melody.equality.EqualNumberOfNotesRequired">
        <property name="equalityTest" ref="contourEqualityTest"/>
    </bean>

    <bean id="inversionEqualityTestWrapper" class="decomposer.analyzer.melody.equality.EqualNumberOfNotesRequired">
        <property name="equalityTest" ref="inversionEqualityTest"/>
    </bean>

    <bean id="intervalsEqualityTestWrapper" class="decomposer.analyzer.melody.equality.EqualNumberOfNotesRequired">
        <property name="equalityTest" ref="intervalsEqualityTest"/>
    </bean>

    <bean id="orderEqualityTestWrapper" class="decomposer.analyzer.melody.equality.EqualNumberOfNotesRequired">
        <property name="equalityTest" ref="orderEqualityTest"/>
    </bean>

    <bean id="rhythmEqualityTestWrapper" class="decomposer.analyzer.melody.equality.EqualNumberOfNotesRequired">
        <property name="equalityTest" ref="rhythmEqualityTest"/>
    </bean>

</beans>